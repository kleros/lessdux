{
  "name": "lessdux",
  "version": "0.6.1",
  "description":
    "A redux toolkit for getting rid of boilerplate and managing async resources painlessly.",
  "keywords": ["redux", "toolkit", "utilities", "utils", "async"],
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": ["es", "lib", "umd"],
  "repository": "https://github.com/kleros/lessdux",
  "author": "Kleros",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prettify": "kleros-scripts prettify",
    "lint": "kleros-scripts lint",
    "start:test": "nwb test --server",
    "test": "nwb test --coverage",
    "test:coveralls": "coveralls < ./coverage/lcov.info",
    "precommit": "kleros-scripts precommit",
    "commitmsg": "kleros-scripts commitmsg",
    "cz": "kleros-scripts cz",
    "clean": "nwb clean-module && nwb clean-demo",
    "build": "nwb build-web-module"
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "expect": "^22.4.0",
    "husky": "^0.14.3",
    "kleros-scripts": "^0.3.3",
    "nwb": "0.21.x",
    "standard-version": "^4.3.0"
  },
  "dependencies": {
    "prop-types": "^15.6.0"
  }
}
